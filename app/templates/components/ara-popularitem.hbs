<section id="team">
        <div class="container">
            <div class="row">
                <h1 class="title text-center wow fadeInDown" data-wow-duration="500ms" data-wow-delay="300ms">Popular</h1>
                <p class="text-center wow fadeInDown" data-wow-duration="400ms" data-wow-delay="400ms">________</p>
                <div id="team-carousel" class="carousel slide wow fadeIn" data-ride="carousel" data-wow-duration="400ms" data-wow-delay="400ms">
                    <!-- Indicators -->
                    <ol class="carousel-indicators visible-xs">
                        <li data-target="#team-carousel" data-slide-to="0" class="active"></li>
                        <li data-target="#team-carousel" data-slide-to="1"></li>
                    </ol>
                    {{#carousel-container transitionInterval=400}}
	                    {{#carousel-body}}
						    {{#carousel-item}}
						      	{{ara-card}}
						    {{/carousel-item}}
						{{/carousel-body}}
	                    <!-- Wrapper for slides -->
	                    <!-- Controls -->
	                    {{#carousel-arrow direction="left"}}
			                <a class="left carousel-control" href="mainHomeCarouse" role="button" data-slide="prev">
  								<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
  								<span class="sr-only">Previous</span>
							</a>
			  			{{/carousel-arrow}}
			  			{{#carousel-arrow direction="right"}}
			  				<a class="right team-carousel-control hidden-xs" href="#team-carousel" data-slide="next"></a>
			  			{{/carousel-arrow}}
			  			<a class="left team-carousel-control hidden-xs" href="#team-carousel" data-slide="prev">left</a>
                    	<a class="right team-carousel-control hidden-xs" href="#team-carousel" data-slide="next">right</a>
		  			{{/carousel-container}}
                 </div> 
            </div>
        </div>
    </section>
    <!--/#team-->
{{yield}}
